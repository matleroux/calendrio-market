<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
      <h1>Create a new place</h1>
      <%= simple_form_for([@owner, @place], url: owner_places_path) do |f| %>
        <%= render partial: 'form', locals: {f: f} %>
      <% end %>
      <br>
      <%= link_to 'Back', owner_places_path %>
    </div>
  </div>
</div>


<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyDANjx3bosEtIyzJaoWs50Wnt6nt_1rmxU"></script>
<% end %>
